<manifest>
    <main-class>driftFisher.driftFisher</main-class>
    <name>Drift Fisher</name>
    <tag-line></tag-line><!--Max of 50 chars-->
    <description>Drift Fishing</description><!--Max of 110 chars-->
    <version>1.0.0</version>
    <compatibility>
        <game>OSRS</game>
    </compatibility>
    <categories>
        <category>FISHING</category>
        <category>HUNTER</category>
    </categories>
    <!--Required to publish on the bot store-->
    <internal-id>driftFisher42</internal-id>
    <!--The rest are optional-->
    <open-source>true</open-source>
    <hidden>false</hidden> <!--If you need to hide it from the bot store for maintenance-->
    <access>public</access>
    <tags>
        <tag>Drift</tag>
        <tag>Drift Fishing</tag>
        <tag>Fishing</tag>
        <tag>Shoal</tag>
    </tags>
    <resources>
        <resource></resource>
    </resources>
</manifest>